{
  "name": "ecko-landing-page",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "description": "Landing Page oficial para lojistas ECKO",
  "scripts": {
    "dev": "vite",
    "build": "npm run build:client && npm run build:server",
